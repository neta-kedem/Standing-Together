    "local-build": "next build && node server",
    "start": "node server -p $PORT",
    "heroku-postbuild": "next build"
  },
  "dependencies": {
    "body-parser": "latest",
    "concurrently": "^4.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^8.0.0",
    "excel4node": "^1.7.2",
    "express": "^4.16.3",
    "fast-xml-parser": "^3.12.19",
    "file-loader": "^1.1.11",
    "file-saver": "^2.0.2",
    "js-cookie": "^2.2.0",
    "mongoose": "^5.0.17",
    "mongoose-paginate": "^5.0.3",
    "multer": "^1.3.1",
    "mysql": "^2.17.1",
    "node-fetch": "latest",
    "node-schedule": "^1.3.2",
    "nodemailer": "^4.6.7",
    "nodemon": "^1.19.1",
    "read-excel-file": "^4.0.1",
    "request": "^2.88.0"
  }
